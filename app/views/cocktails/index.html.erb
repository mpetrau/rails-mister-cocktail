<% @title = "Welcome to Drinkalicious!"  %>
<% @subtitle = "Your private mixer"  %>
<%= render 'banner' %>

<div class="container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <ul class="list-unstyled margin-top">
        <% @cocktails.each do |cocktail|  %>
        <li class ="card"><%= link_to cocktail.name, cocktail_path(cocktail) %></li><hr>
        <% end %>
        <ul>
    </div>
  </div>
</div>
